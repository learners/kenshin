{
  "name": "kenshin",
  "version": "1.0.0",
  "description": "React UI Components",
  "keywords": [
    "umi",
    "react",
    "frontend",
    "component"
  ],
  "homepage": "https://github.com/learners",
  "author": "learners",
  "contributors": [
    {
      "name": "richard",
      "email": "richard.lam@my.com",
      "url": "https://github.com/yibhou"
    },
    {
      "name": "xyx650",
